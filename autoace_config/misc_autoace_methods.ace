//
//
// WormBase methods file
//
// all methods should be stored here
//
// GFF_source  = program/method/experiment used to determine what the thing is (Free text)
// GFF_feature = what the thing actually is (should try to use SO_terms where possible)
//
// E.g. a tRNA gene predicted by the program tRNA-scan would be GFF_source = 'tRNA-scan'
// and GFF_feature = 'tRNA'


///////////////////////////////////////////////////////////////////////////
//									 //
//         TO ADD A NEW METHOD YOU HAVE TO DO 3 THINGS	   	         //
//	   -------------------------------------------			 //
//									 //
// i)   FIND A RELEVANT PLACE IN THE OBJECTS SECTION AND		 //
//      POPULATE THE NEW METHOD						 //
//									 //
// ii)  GO TO THE Fmap SECTION (if it is to be mapped) AT THE BOTTOM AND //
//      DECIDED WHERE IT SHOULD GO ON THE Fmap VIA Right_priority        //
//									 // 
// iii) IF GFF3 DUMPING FOR THE OBJECT(s) ASSOCIATED WITH THE METHOD IS	 // 
//      NOT SIMPLE (i.e. HIERARCHICAL), YOU WILL NEED TO CONNECT IT TO   //
//      THE APPROPRIATE Ace2SO OBJECT				         //
//								         //
///////////////////////////////////////////////////////////////////////////

				//////////////////////////////
				//		            //
				//   OBJECTS SECTION        //
				//		            //
				//////////////////////////////

/////////////////////
// ?Ace2SO objects //
/////////////////////

Ace2SO : "curated_CDS_ace2so"
Coding_region "SO:0000316" // CDS
SO_parent_obj_tag Corresponding_transcript
No_span

Ace2SO : "coding_transcript_ace2so"
SO_parent_obj_tag Gene
Exons "SO:0000147" // exon
Introns "SO:0000188" // intron
SO_children

Ace2SO : "predicted_CDS_ace2so"
Coding_region "SO:0000316" // CDS
No_span

Ace2SO : "nc_transcript_ace2so"
SO_parent_obj_tag "Gene"
Exons "SO:0000147" // exon

Ace2SO : "transposon_gene_ace2so"
SO_parent_obj_tag "Corresponding_transposon"
SO_children

Ace2SO : "transposable_element_ace2so"
SO_children

Ace2SO : "gene_ace2so"
SO_children


