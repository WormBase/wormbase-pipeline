# This config file contains the list of objects and databases and 
# required criteria for making a complete set of objects for
# making autoace automatically

#Fields are key=value pairs, with following keys valid -
#db		- the database name to dump from,
#class	- the class to dump,
#query	- acedb query to run to specify set,
#delete	- tags to delete before dumping,
#tag	- only dump this tag data from specified class,
#follow	- dumps the specified class data for this tag and follows the tag to dump that object too.
#path	- specifies the location of the database to be dumped. Must be a method in Wormbase.pm

path=primaries
db=japonica	file=japonica_CDS.ace		class=CDS		query=(Species="Caenorhabditis japonica" AND Method)	delete=RNASeq_FPKM	delete=Confidential_remark	delete=Corresponding_protein	delete=Protein_id	delete=Associated_feature
db=japonica	file=japonica_Pseudogene.ace	class=Pseudogene	query=(Species="Caenorhabditis japonica")	delete=RNASeq_FPKM	delete=Confidential_remark
db=japonica	file=japonica_Transcript.ace	class=Transcript	query=(Species="Caenorhabditis japonica" AND Method AND Method!="Coding_transcript")	delete=RNASeq_FPKM	delete=Confidential_remark
db=japonica	file=japonica_Sequence.ace	class=Sequence		query=(Species="Caenorhabditis japonica")	delete=Homol_data	delete=Feature_data	delete=Homol_homol	delete=Confidential_remark	delete=Matching_CDS	delete=Feature_object
db=japonica	file=japonica_DNA.ace		class=DNA
db=japonica	file=japonica_Genetic_code.ace	class=Genetic_code
db=japonica	file=japonica_LongText.ace	class=LongText
db=japonica	file=japonica_Feature.ace	class=Feature
db=japonica	file=japonica_Feature_data.ace	class=Feature_data
db=japonica	file=japonica_EST_features.ace	class=Sequence	query=(Method = "EST_japonica")	follow=Feature_data
db=japonica	file=japonica_Trinity_features.ace	class=Sequence	query=(Method = "RNASeq_trinity")	follow=Feature_data
db=japonica	file=japonica_NDB_features.ace	class=Sequence	query=(Method = "NDB")	follow=Feature_data

db=geneace	file=japonica_Genes.ace		class=Gene		query=(Species="Caenorhabditis japonica")	delete=Reference
db=geneace	file=japonica_Variation.ace		class=Variation		query=(Species="Caenorhabditis japonica")
db=geneace	file=japonica_Sequence_collection.ace		class=Sequence_collection		query=(Species="Caenorhabditis japonica")
db=geneace	file=japonica_Species.ace		class=Species		query=("Caenorhabditis japonica")
db=geneace	file=japonica_Analysis.ace	class=Analysis	format="Reference WBPaper\d{8}"	format="Conducted_by WBPerson\d{1,5}"
db=geneace	file=japonica_Condition.ace	class=Condition

db=citace	file=japonica_Oligo_set.ace		class=Oligo_set		query=(Species="Caenorhabditis japonica")	delete=Overlaps_CDS	delete=Overlaps_transcript	delete=Overlaps_pseudogene
db=citace	file=japonica_Microarray_results.ace		class=Microarray_results	query=(Species="Caenorhabditis japonica")	delete=Gene	delete=CDS	delete=Transcript	delete=Pseudogene
db=citace	file=japonica_Microarray_experiment.ace	class=Microarray_experiment	query=(Species="Caenorhabditis japonica")	format="Reference WBPaper\d{8}"
db=citace	file=caltech_RNAi.ace	class=RNAi	query=(Species="Caenorhabditis japonica")	delete=Inhibits	delete=Homol	format="Reference WBPaper\d{8}"	format="Phenotype WBPhenotype:\d{7}"

db=config	file=misc_autoace_methods.ace

path=misc_static
db=misc	file=misc_SO_terms.ace
